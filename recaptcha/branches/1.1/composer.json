{
  "name": "pollen-solutions/recaptcha",
  "description": "Pollen Recaptcha Component - Recaptcha integrator",
  "homepage": "https://www.presstify.com/pollen-solutions/recaptcha/",
  "keywords": [
    "pollen-solutions",
    "component",
    "recaptcha"
  ],
  "time": "2021-02-25",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "google/recaptcha": "^1.2",
    "pollen-solutions/field": "^1.0",
    "pollen-solutions/form": "^1.0",
    "pollen-solutions/http": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Recaptcha\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Recaptcha\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
