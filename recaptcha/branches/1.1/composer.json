{
  "name": "pollen-solutions/recaptcha",
  "description": "Pollen Recaptcha Component - Recaptcha integrator",
  "homepage": "https://www.presstify.com/pollen-solutions/recaptcha/",
  "keywords": [
    "pollen-solutions",
    "component",
    "recaptcha"
  ],
  "time": "2021-06-02",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "google/recaptcha": "^1.2",
    "pollen-solutions/field": "^1.0",
    "pollen-solutions/http": "^1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Recaptcha\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Recaptcha\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/asset" : "Pollen Asset Component - Manage CSS styles, JS scripts, ...",
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container.",
    "pollen-solutions/form": "Pollen Form Component - Forms generator and processor."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
